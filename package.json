{
  "name": "spotifind",
  "version": "1.0.0",
  "description": "An easier way to manage your Spotify content",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikejancar/Spotifind.git"
  },
  "keywords": [
    "spotify",
    "manage"
  ],
  "author": "Michael Jancar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikejancar/Spotifind/issues"
  },
  "homepage": "https://github.com/mikejancar/Spotifind#readme",
  "dependencies": {
    "angular2": "^2.0.0-beta.17",
    "angular2-in-memory-web-api": "0.0.7",
    "bootstrap": "^3.3.6",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.6",
    "systemjs": "^0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  }
